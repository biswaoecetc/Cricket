CREATE  TABLE IF NOT EXISTS CricketDB.CRICKET_TEAM (
  team_id VARCHAR(20) NOT NULL,
  team_name VARCHAR(150) NOT NULL ,
  score VARCHAR(6) ,
  date_of_match DATE ,
  PRIMARY KEY (team_id) );

INSERT INTO CRICKET_TEAM VALUES('123', 'ABC', '230', '2019-04-31');

ALTER TABLE CRICKET_TEAM
ADD team_id_played VARCHAR(20);

